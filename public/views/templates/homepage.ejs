<!DOCTYPE html>
<html lang="en">
	<%- include('../includes/head.ejs')%>
	<body>
		<section class="journal-head">
			<h1>Welcome</h1>
		</section>
		<section class="journal-content">
			<% for (let i = 0; i < allEntries.length; i++) { %>
			<div>
				<h2><%= allEntries[i].title%></h2>
				<p><%= allEntries[i].content%></p>
				<p><%= allEntries[i].date%></p>
			</div>
			<%}%>
		</section>
		<section class="add-content"></section>
	</body>
</html>
